{"full_text": " ▲ [${upspeed enp3s0f2}] ", "color":"\#ff0000"},
{"full_text": " ▼ [${downspeed enp3s0f2}] ", "color":"\#00fff2"},
{"full_text": " ↑ [${uptime}] ","color": "\#dc322f"},
{"full_text": " [${time %a %d %b} ${time %H:%M:%S}] ","color": "\#2fdc57"}],